{
  "extends": "semistandard",
  
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "always",
      "objects": "always",
      "imports": "always",
      "exports": "always",
      "functions": "never"
    },
    "quotes": ["error", "double", { "avoidEscape": true, "allowTemplateLiterals": true }]
  }
}
