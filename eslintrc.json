{
  "extends": "semistandard",
  
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "never"
    }],
    "quotes": ["error", "double", { "avoidEscape": true, "allowTemplateLiterals": true }]
  }
}
